@font-face {
  font-family: 'Muller';
  src: local("Muller-Regular"), url("../fonts/MullerRegular.woff2") format("woff2"), url("../fonts/MullerRegular.woff") format("woff");
  font-weight: 400;
  font-style: normal;
  font-display: swap;
}

@font-face {
  font-family: 'Muller';
  src: local("Muller"), url("../fonts/MullerLight.woff2") format("woff2"), url("../fonts/MullerLight.woff") format("woff");
  font-weight: 300;
  font-style: normal;
  font-display: swap;
}

@font-face {
  font-family: 'Muller';
  src: local("Muller"), url("../fonts/MullerMedium.woff2") format("woff2"), url("../fonts/MullerMedium.woff") format("woff");
  font-weight: 500;
  font-style: normal;
  font-display: swap;
}

@font-face {
  font-family: 'Muller';
  src: local("Muller"), url("../fonts/MullerBold.woff2") format("woff2"), url("../fonts/MullerBold.woff") format("woff");
  font-weight: 700;
  font-style: normal;
  font-display: swap;
}

html {
  box-sizing: border-box;
}

*,
*::after,
*::before {
  box-sizing: inherit;
}

a {
  color: inherit;
  text-decoration: none;
}

img {
  max-width: 100%;
}

/* global */

body {
  min-width: 1200px;
  font-family: 'Muller';
  font-weight: 400;
  background-color: #E5E5E5;
}

.container {
  max-width: 1200px;
  margin: 0 auto;
  padding: 0 15px;
}

h3, p {
  margin: 0;
}

.flex {
  display: flex;
}

.btn-reset {
  padding: 0;
  background-color: transparent;
  border: none;
  cursor: pointer;
}

.list-reset {
  margin: 0;
  padding: 0;
  list-style: none;
}

.background-elements {
  background-color: #fff;
}

.general-radius {
  border-radius: 15px;
  overflow: hidden;
}

.section {
  padding: 35px 0;
}

.section-title {
  margin: 0;
  margin-bottom: 20px;
  padding-left: 40px;
  font-weight: 400;
  font-size: 40px;
  line-height: 40px;
  color: #333333;
}

.button {
  padding: 13px 20px;
  font-size: 16px;
  line-height: 16px;  
  font-weight: 400;
  border: 1px solid #CC9933;
  border-radius: 10px;
  color: #CC9933;
}

.button:hover {
  color: #ffffff;
  background-color: #CC9933;
}

.button:active {
  color: #ffffff;
  background-color: #A47312;
}

a:hover {
  color: #CC9933;
}

a:active {
  color: #A47312;
}

/* header */

.header {
  padding-top: 30px;
  margin-bottom: 35px;
}

.header-part-1 {
  align-items: center;
  margin-bottom: 30px;
}

.logo-link {
  margin-right: 40px;
}

.tel-link {
  margin-right: auto;
  font-weight: 500;
  font-size: 20px;
  line-height: 20px;
  color: #666666;
}

.header-log-in {
  padding-left: 32px;
  font-weight: 400;
  font-size: 16px;
  line-height: 16px;
  color: #CC9933;
  background-image: url('../img/head-log-in.svg');
  background-repeat: no-repeat;
  background-position: left top;
  background-size: 24px;
}

.header-part-2 {
  height: 70px;
  padding: 0 45px;
  align-items: center;
}

.header-nav {
  margin-right: auto;
  font-weight: 400;
  font-size: 16px;
  line-height: 16px;
  color: #666666;
}

.header-list-item:not(:last-child) {
  margin-right: 45px;
}



.header-list-buttons-item:not(:last-child) {
  margin-right: 40px;
}

/* special-offers */

.special-offers-item-title {
  margin: 0;
  margin-bottom: 15px;
  font-weight: 700;
  font-size: 40px;
  line-height: 40px;
  color: #FFFFFF;
}

.special-offers-list-item:last-child > .special-offers-item-title {
  font-weight: 700;
  font-size: 60px;
  line-height: 60px;
}

.special-offers-price {
  margin-bottom: auto;
  font-weight: 400;
  font-size: 20px;
  line-height: 20px;
  color: #ffffff;
  opacity: 0.8;
}

.special-offers-list-item:last-child > .special-offers-price {
  font-weight: 400;
  font-size: 30px;
  line-height: 30px;
}

.special-offers-link {
  padding-right: 20px;
  font-weight: 400;
  font-size: 16px;
  line-height: 16px;
  color: #F0BF5F;
  background-image: url("../img/link-arrow.svg");
  background-size: 11px;
  background-repeat: no-repeat;
  background-position: right;
}

.special-offers-list {
  flex-flow: column wrap;
  height: 540px;
}

.special-offers-list-item {
  width: 573px;
  height: 258px;
  margin-bottom: 24px;
  margin-right: 24px;
  padding: 40px 45px;
  flex-direction: column;
  align-items: flex-start;
}

.list-item-1 {
  background-image: linear-gradient(90deg, rgba(48, 64, 89, 0.84) -2.9%, rgba(53, 65, 83, 0) 65.7%), url("../img/maldivy.jpg");
  background-repeat: no-repeat;
  background-size: cover;
  background-position: center;
}

.list-item-2 {
  background-image: linear-gradient(90deg, rgba(48, 64, 89, 0.84) -2.9%, rgba(53, 65, 83, 0) 65.7%), url("../img/krit.jpg");
  background-repeat: no-repeat;
  background-size: cover;
  background-position: center;
}

.list-item-3 {
  background-image: url("../img/hotel-room.jpg");
  background-repeat: no-repeat;
  background-size: cover;
  background-position: center;
}


.special-offers-list-item:nth-child(3n) {
  min-height: 540px;
  margin-right: 0;
  padding-top: 215px;
  padding-right: 230px;
}

.special-offers-list-item:nth-child(3n-1) {
  margin-bottom: 0;
}

/* about-us */

.about-us-content {
  width: 888px;
}

.about-us-text {
  font-weight: 400;
  font-size: 16px;
  line-height: 200%;
  color: #000000;
}

/* services */

.services-list {
  flex-wrap: wrap;
  align-content: stretch;
}

.services-list-item {
  min-height: 330px;
  margin-bottom: 24px;
  background-color: #FFFFFF;
}

.services-list-item:not(:nth-child(2n)) {
  margin-right: 24px;
}

.service-img {
  margin-bottom: 26px;
}

.service-list-item-container {
  width: 402px;
  height: 104px;
  margin-left: 45px;
}

.service-item-title {
  display: inline-block;
  padding-right: 18px;
  font-weight: 500;
  font-size: 20px;
  line-height: 200%;
  color: #CC9933;
  background-image: url(../img/arrow-service.svg);
  background-repeat: no-repeat;
  background-position: right;
  background-size: 11px;
}

.service-item-title-text {
  font-weight: 400;
  font-size: 16px;
  line-height: 200%;
  color: #000000;
}

/* advantages */

.advantages-list {
  flex-wrap: wrap;
}

.advantages-list-item {
  width: 276px;
  padding: 40px 45px 32px 45px;
  background-color: #ffffff;
  margin-right: 22px;
  margin-bottom: 22px;
}

.advantages-list-item:nth-child(4n) {
  margin-right: 0;
}

.advantage-img {
  width: 66px;
  height: 66px;
  margin-bottom: 10px;
}

.advantage-text {
  font-weight: 400;
  font-size: 16px;
  line-height: 200%;
}

/* offers */

.offers-list {
  flex-wrap: wrap;
}

.offers-list-item {
  width: 374px;
  margin-right: 24px;
  margin-bottom: 24px;
  background-color: #fff;
}

.offers-list-item:nth-child(3n) {
  margin-right: 0;
}

.offer-list-item-container {
  padding: 32px 45px 45px 45px;
}

.offer-descr {
  margin-bottom: 16px;
}

.offer-from {
  margin-right: 4px;
  font-weight: 400;
  font-size: 16px;
  line-height: 16px;
  color: #999999;
}

.offer-price {
  margin-right: 4px;
  font-weight: 700;
  font-size: 20px;
  line-height: 20px;
  color: #000000;
}

.offer-night {
  margin-right: auto;
  font-weight: 400;
  font-size: 16px;
  line-height: 16px;
  color: #000000;
}

.offer-stars-img {
  width: 14px;
  height: 14px;
}

.offer-name {
  font-weight: 400;
  font-size: 16px;
  line-height: 16px;
  color: #000000;
  margin-bottom: 16px;
}

.offer-location {
  margin-bottom: 20px;
  font-weight: 400;
  font-size: 16px;
  line-height: 16px;
  color: #999999;
}

.item-see-more {
  padding: 27px 0;
  flex-direction: column;
  justify-content: end;
  align-items: center;
  background-image: url("../img/offer-last-card.jpg");
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
}

.offer-see-more {
  padding-right: 18px;
  font-weight: 500;
  font-size: 20px;
  line-height: 200%;
  color: #ffffff;
  background-image: url(../img/arrow-white.svg);
  background-repeat: no-repeat;
  background-position: right;
  background-size: 11px;
}

/* tour-form */

.tour-search-content {
  background-color: #E9E9E9;
  border-radius: 15px;
  padding: 45px;
}

.tour-search-title {
  padding-left: 0;
}

.form-container {
  padding: 32px 28px;
  background-color: #FFFFFF;
  border-radius: 15px;
  align-items: center;
}

.form-list {
  margin-right: 51px;
}

.form-item {
  flex-direction: column;
}

.form-item:not(:last-child) {
  margin-right: 41px;
}

.input-title {
  font-weight: 400;
  font-size: 12px;
  line-height: 12px;
  color: #000000;
  margin-bottom: 15px;
}

.form-selects {
  width: 169px;
  font-weight: 400;
  font-size: 16px;
  line-height: 16px;
  border: none;
}

.input-date {
  width: 121px;
  font-weight: 400;
  font-size: 16px;
  line-height: 16px;
  border: none;
}

.input-counter {
  width: 121px;
  font-weight: 400;
  font-size: 16px;
  line-height: 16px;
  border: none;
}

.btn-form {
  width: 108px;
}

/* contacts */

.contacts-descr {
  padding: 45px;
  background-color: #FFFFFF;
  border-radius: 15px; 
}

.contacts-left {
  min-width: 375px;
  margin-right: 52px;
}

.contacts-list {
  flex-direction: column;
  align-items: flex-start;
  margin-bottom: 45px;
}

.contacts-item {
  padding-bottom: 25px;
  margin-bottom: 25px;
  border-bottom: 1px solid #E9E9E9;
}

.contacts-item:last-child {
  margin-bottom: 0;
}

.contacts-item-div {
  display: flex;
  flex-direction: column;
}

.contacts-item-title {
  min-width: 150px;
  font-weight: 700;
  font-size: 20px;
  line-height: 20px;
  color: #000000;
}

.contacts-item-p {
  font-weight: 400;
  font-size: 16px;
  line-height: 150%;
  color: #000000;
}

.contact-links {
  font-weight: 400;
  font-size: 16px;
  line-height: 150%;
  color: #CC9933;
}

.contact-mail-descr {
  font-weight: 400;
  font-size: 16px;
  line-height: 150%;
  color: #999999;
}

.btn-contacts {
  width: 213px;
  height: 70px;
}

.contacts-right>img {
  width: 100%;
}

/* footer */

.footer-content {
  padding: 45px;
  margin-bottom: 30px;
  background-color: #333333;
  border-radius: 15px;
  background-image: url(../img/footer-sun.svg);
  background-repeat: no-repeat;
  background-position: 739px bottom;
}

.footer-left {
  width: 302px;
  margin-right: 125px;
}

.footer-left-list {
  flex-direction: column;
  align-items: flex-start;
}

.footer-left-item:not(:last-child) {
  margin-bottom: 40px;
}

.footer-center {
  margin-right: auto;
}

.footer-center-list {
  flex-direction: column;
  align-items: flex-start;
}

.footer-center-item:not(:last-child) {
  margin-bottom: 15px;
}

.footer-list-flex {
  flex-direction: column;
}

.social-links:not(:last-child) {
  margin-bottom: 20px;
}

.social-links-svg:hover > svg > path {
  fill: #CC9933;
}

.social-links-svg:active > svg > path {
  fill: #A47312;
}

.footer-text {
  font-weight: 400;
  font-size: 16px;
  line-height: 190%;
  color: #ffffff;
}